Module m = myRead("$$url$$", "$$name$$", false)
if (!null m) {
	if (load(m, view "$$view$$")) {
		exportModuleToCSV(m, "$$file$$")
		exportModuleMetaData(m, "$$file$$.mmd", null)
	} else {
		throw("Cannot find view $$view$$ in module " name(m) ".")
	}
} else {
	throw("Cannot load module $$url$$$$name$$.")
}
